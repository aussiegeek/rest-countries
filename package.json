{
  "name": "rest-countries",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "chromatic": "chromatic",
    "eject": "react-scripts eject",
    "graphql:types": "graphql-codegen --config codegen.yml",
    "lint": "yarn eslint --max-warnings=0 --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn eslint --max-warnings=0 --ext .js,.jsx,.ts,.tsx --fix .",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "stylelint": "stylelint 'src/**/*.tsx'",
    "test": "react-scripts test --env=jsdom-fourteen",
    "typescript:check": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": "stylelint --fix",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "package.json": "sort-package-json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@apollo/client": "3.1.4",
    "@babel/core": "7.12.3",
    "@reach/router": "1.3.4",
    "@types/node": "12.12.68",
    "@types/reach__router": "1.3.5",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/styled-components": "5.1.3",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.11.1",
    "graphql": "15.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "react-scripts": "3.4.3",
    "styled-components": "5.2.0",
    "typescript": "3.9.7",
    "webpack": "4.42.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.18.0",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/schema-ast": "1.17.8",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.7",
    "@graphql-tools/mock": "6.2.4",
    "@graphql-tools/schema": "6.2.4",
    "@storybook/addon-actions": "6.0.26",
    "@storybook/addon-links": "6.0.26",
    "@storybook/addon-viewport": "6.0.26",
    "@storybook/addons": "6.0.26",
    "@storybook/preset-create-react-app": "3.1.4",
    "@storybook/react": "6.0.26",
    "@testing-library/dom": "7.26.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.8",
    "@types/faker": "5.1.2",
    "@types/jest": "24.9.1",
    "@types/react-test-renderer": "16.9.3",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "@yarnpkg/pnpify": "2.3.3",
    "apollo": "2.30.3",
    "babel-plugin-macros": "2.8.0",
    "chromatic": "5.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "10.0.0",
    "eslint-config-prettier": "6.13.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jest-formatting": "2.0.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
